{
  "name": "tonomy-communication",
  "version": "0.0.1",
  "description": "",
  "author": "Rebal Alhaqash",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "esm": "cross-env NODE_OPTIONS=\"--experimental-vm-modules\"",
    "build": "yarn run esm nest build && yarn run build:post",
    "build:post": "yarn run esm node add-js-extension.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn run esm nodemon",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn run esm lint --fix",
    "test": "yarn run esm jest",
    "test:unit": " yarn run test --selectProjects \"Unit tests\"",
    "test:e2e": "yarn run test --selectProjects \"End-to-end tests\"",
    "updateSdkVersion": "./update_sdk_version.sh"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.3",
    "@nestjs/core": "^10.2.3",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/platform-express": "^10.2.3",
    "@nestjs/platform-socket.io": "^10.2.3",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/websockets": "^10.2.3",
    "@tonomy/tonomy-id-sdk": "../",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "hcaptcha": "^0.1.1",
    "helmet": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^10.4.2",
    "jest": "^29.6.4",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
