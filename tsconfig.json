{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Preserve",
    "declaration": true,
    "removeComments": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true
  }
}